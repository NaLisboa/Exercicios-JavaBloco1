package br.com.generation.exercicios_aula02;

import java.util.Scanner;

public class Exercicio01_0410 {

	public static void main(String[] args) {
		
		int n1, n2, n3;
		
		Scanner leia = new Scanner(System.in);
		System.out.println("Escreva 3 números: ");
		
		n1 = leia.nextInt();
		n2 = leia.nextInt();
		n3 = leia.nextInt();
		
		if (n1 > n2 && n1 > n3) {
			System.out.println("Esse é o maior número: " + n1);
		}
		
		else if (n2 > n1 && n2 > n3) {
			System.out.println("Esse é o maior número: " + n2);
		}
		else {
			System.out.println("Esse é o maior número: " + n3);
		}
		
	}

}
