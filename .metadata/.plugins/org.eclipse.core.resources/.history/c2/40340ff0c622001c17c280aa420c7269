package br.com.generation.aula01;

public class exemplovariavel {

	public static void main(String[] args) {
		
		//declaração de variáveis
		
		int var1; var2;
		
		var1 = 1000;

		
		System.out.println("Valor da variável 1: " + var1);
		
		var2 = var1/ 2; //o total vai ser o valor do var2
		
		System.out.println("Valor da variável 2: " + var2);
	}

}
